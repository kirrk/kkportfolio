<h2>My Portfolio</h2>

<% @projects.each do |project| %>
<div class="project" id="<%= dom_id(project) %>">
  <h3><%= project.name %></h3>
  <p><%= project.technologies_used %></p>
  <div class-"nav">
    <%= link_to 'Show', project %> |
    <%= link_to 'Edit', edit_project_path(project) %> |
    <%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are youre sure?'} %>
  </div>
</div>
<% end %>

<br />
<%= link_to 'New project', new_project_path, remote: true, id: :new_link %>

